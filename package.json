{
  "name": "nuxt-404",
  "version": "0.1.0",
  "description": "Nuxt.js module to render trendy 404 pages.",
  "main": "module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt dev ./example",
    "lint": "eslint --ext .js,.vue ."
